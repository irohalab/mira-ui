{
  "/api/v2/**": {
    "target": "http://127.0.0.1:9091",
    "secure": false,
    "pathRewrite": {
      "^/api/v2": ""
    }
  },
  "/api/**": {
    "target": "http://127.0.0.1:5000",
    "secure": false
  },
  "/pic/**": {
    "target": "http://127.0.0.1:8000",
    "secure": false
  },
  "/video/**": {
    "target": "http://127.0.0.1:8000",
    "secure": false
  },
  "/rt-log": {
    "target": "http://127.0.0.1:8083/rt-log/",
    "secure": false,
    "changeOrigin": true,
    "ws": true
  }
}
